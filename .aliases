#!/usr/bin/env bash

# Easier navigation: .., ..., ...., .....
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# Shortcuts
alias desktop="cd ~/Desktop"
alias sites="cd ~/Sites"

# Project Starters
alias create-my-react-app="bash ~/bin/create-my-react-app.sh"
alias create-my-mern-app="bash ~/bin/create-my-mern-app.sh"
alias create-wp="bash ~/bin/create-wp.sh"
alias install-wp="sh bin/setup.sh"

# Docker
alias dcwp='docker-compose exec --user www-data phpfpm wp'
